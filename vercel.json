{
  "rewrites": [
    {
      "source": "/api/voices",
      "destination": "/api/proxy/voices"
    },
    {
      "source": "/api/generate",
      "destination": "/api/proxy/generate"
    },
    {
      "source": "/api/preview",
      "destination": "/api/proxy/preview"
    },
    {
      "source": "/api/jobs/:jobId",
      "destination": "/api/proxy/jobs/:jobId"
    },
    {
      "source": "/api/filter_voices",
      "destination": "/api/proxy/filter_voices"
    },
    {
      "source": "/api/transcribe_upload",
      "destination": "/api/proxy/transcribe_upload"
    },
    {
      "source": "/api/transcribe_generated",
      "destination": "/api/proxy/transcribe_generated"
    },
    {
      "source": "/api/cleanup",
      "destination": "/api/proxy/cleanup"
    },
    {
      "source": "/audio/:path*",
      "destination": "/api/audio-proxy/:path*"
    }
  ]
}
